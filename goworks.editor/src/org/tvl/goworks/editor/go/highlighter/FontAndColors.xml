<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!--
        COMMON
    -->
    <fontcolor name="identifier" default="identifier"/>
    <fontcolor name="keyword" default="keyword"/>
<!--    <fontcolor name="definition" foreColor="FF2B91AF" />-->
<!--    <fontcolor name="reference" foreColor="FF008800">
        <font style="bold" />
    </fontcolor>-->
    <fontcolor name="comment" default="comment" />
    <fontcolor name="stringliteral" default="string" />
    <fontcolor name="number" default="number" />
    <fontcolor name="whitespace" default="whitespace"/>
<!--    <fontcolor name="operator" default="operator" />-->

    <!--
        GO SEMANTICS
    -->

    <fontcolor name="stringliteralescape" default="stringliteral">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="stringliteralinvaildescape" default="stringliteralescape" waveUnderlined="FFCE7B00" />

    <fontcolor name="packageUse" default="identifier" />
    <fontcolor name="packageDeclaration" default="packageUse" />

    <fontcolor name="constUse" default="identifier" foreColor="FF009900">
        <font style="italic" />
    </fontcolor>

    <fontcolor name="globalConstUse" default="constUse" />
    <fontcolor name="globalConstDeclaration" default="globalConstUse" />

    <fontcolor name="localConstUse" default="constUse" />
    <fontcolor name="localConstDeclaration" default="localConstUse" />

    <fontcolor name="funcUse" default="identifier" />
    <fontcolor name="funcDeclaration" default="funcUse" />

    <fontcolor name="methodUse" default="identifier" />
    <fontcolor name="methodDeclaration" default="methodUse" />

    <fontcolor name="typeUse" default="identifier" foreColor="FF2B91AF" />
    <fontcolor name="typeDeclaration" default="typeUse" />

    <fontcolor name="structUse" default="typeUse" />
    <fontcolor name="structDeclaration" default="structUse" />

    <fontcolor name="interfaceUse" default="typeUse" />
    <fontcolor name="interfaceDeclaration" default="interfaceUse" />

    <fontcolor name="builtinTypeUse" default="typeUse" foreColor="FF0000FF" />
    <fontcolor name="builtinConstUse" default="constUse" foreColor="FF0000FF" />
    <fontcolor name="builtinFunctionUse" default="funcUse" />

    <fontcolor name="parameterUse" default="identifier" />
    <fontcolor name="parameterDeclaration" default="parameterUse" />

    <fontcolor name="returnParameterUse" default="parameterUse" />
    <fontcolor name="returnParameterDeclaration" default="returnParameterUse" />

    <fontcolor name="varUse" default="identifier" />

    <fontcolor name="globalVarUse" default="varUse" foreColor="FF009900">
        <font style="italic" />
    </fontcolor>
    <fontcolor name="globalVarDeclaration" default="globalVarUse" />

    <fontcolor name="fieldVarUse" default="varUse" foreColor="FF009900" />
    <fontcolor name="fieldVarDeclaration" default="fieldVarUse" />

    <fontcolor name="localVarUse" default="varUse" />
    <fontcolor name="localVarDeclaration" default="localVarUse" />

    <fontcolor name="labelUse" default="identifier" />
    <fontcolor name="labelDeclaration" default="labelUse" />

    <fontcolor name="unresolvedIdentifier" default="identifier" waveUnderlined="FF9900000" />

    <!-- Slate Blue -->
<!--    <fontcolor name="parserrule" foreColor="FF6A5ACD" />-->
    <!-- Dark Blue -->
<!--    <fontcolor name="lexerrule" foreColor="FF00008B" />-->
    <!-- Teal -->
